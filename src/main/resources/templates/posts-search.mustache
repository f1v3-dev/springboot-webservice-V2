{{>layout/header}}

<div id="posts_list">
    <table id="table" class="table table-horizontal">
    <thead id="thead">
    <tr>
        <th>번호</th>
        <th>제목</th>
        <th>작성자</th>
        <th>등록일</th>
    </tr>
    </thead>
    <tbody id="tbody">
    {{#searchList}}
        <tr>
            <td>{{id}}</td>
            <td><a href="/posts/read/{{id}}">{{title}}</a></td>
            <td>{{author}}</td>
            <td>{{modifiedDate}}</td>
        </tr>
    {{/searchList}}
    </tbody>
        </table>
        <div class="text-right">
            <a href="/posts/save" role="button" class="btn btn-primary bi bi-pencil-fill"> 글 등록</a>
        </div>
</div>

{{>layout/footer}}